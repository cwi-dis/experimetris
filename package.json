{
  "name": "experimetris",
  "productName": "Experimetris",
  "version": "1.9.8",
  "description": "An Electron application to run cognitive load studies using a version of Tetris",
  "main": "bundle/main.js",
  "repository": "https://github.com/troeggla/experimetris",
  "author": "Thomas RÃ¶ggla",
  "license": "BSD-2-Clause",
  "scripts": {
    "build": "webpack",
    "start": "electron bundle/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder && cd dist/mac && tar cvf Experimetris.tar Experimetris.app/ && gzip Experimetris.tar",
    "lint": "eslint --ext=ts,tsx,js,jsx ."
  },
  "build": {
    "appId": "nl.cwi.dis.experimetris",
    "productName": "Experimetris",
    "artifactName": "Experimetris.${ext}",
    "publish": null,
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    },
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/yt-player": "^3.4.0",
    "classnames": "^2.2.6",
    "css-loader": "^5.2.4",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "style-loader": "^1.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.0.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.28.0"
  },
  "dependencies": {
    "ajv": "^7.0.3",
    "bulma": "^0.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "yt-player": "^3.4.0"
  }
}
